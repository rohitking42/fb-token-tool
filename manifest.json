{
  "manifest_version": 3,
  "name": "Karan Token Extractor",
  "version": "1.0",
  "description": "Get EAAB Token from Kiwi Browser",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://*.facebook.com/*",
    "https://*.business.facebook.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
